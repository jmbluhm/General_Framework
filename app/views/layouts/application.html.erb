<!DOCTYPE html>
<html>
<head>
  <title>GeneralFramework</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<nav class="light-gray-text navbar navbar-default navbar-static-top" role="navigation">
	  <div class='container'>
	    <div class="navbar-header">
	      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#main-nav-collapse">
	        <span class="sr-only">Toggle navigation</span>
	        <span class="icon-bar"></span>
	        <span class="icon-bar"></span>
	        <span class="icon-bar"></span>
	      </button>
		  <a class="navbar-brand logo" rel="home" href="#" title="Logo Title">
		         <%= image_tag asset_path("assets/navbarlogo.png"), class: "image-responsive"%>
				 
				 
		     </a>
	      
	    </div>
	    <div class="collapse navbar-collapse" id="main-nav-collapse">
	      <ul class="nav navbar-nav navbar-right navbar-text">
	        <li><%= link_to "About", about_path, class: 'navbar-brand' %></li>
	      </ul>
	      <ul class="nav navbar-nav navbar-right navbar-text">
	        <li><%= link_to "Services", services_path, class: 'navbar-brand' %></li>
	      </ul>
		  <ul class="nav navbar-nav navbar-right navbar-text">
		  	<%= link_to 'Home', root_path, class: 'navbar-brand' %>
		  </ul>
	    </div><!-- /.navbar-collapse -->
	  </div>
	</nav>
			
					
<div class="container">
	<%= yield %>
</div>
</body>
</html>
